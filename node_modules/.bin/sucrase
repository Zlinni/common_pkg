#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/peropero/Desktop/job/common_pkg/node_modules/sucrase/bin/node_modules:/mnt/c/Users/peropero/Desktop/job/common_pkg/node_modules/sucrase/node_modules:/mnt/c/Users/peropero/Desktop/job/common_pkg/node_modules:/mnt/c/Users/peropero/Desktop/job/node_modules:/mnt/c/Users/peropero/Desktop/node_modules:/mnt/c/Users/peropero/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/peropero/Desktop/job/common_pkg/node_modules/sucrase/bin/node_modules:/mnt/c/Users/peropero/Desktop/job/common_pkg/node_modules/sucrase/node_modules:/mnt/c/Users/peropero/Desktop/job/common_pkg/node_modules:/mnt/c/Users/peropero/Desktop/job/node_modules:/mnt/c/Users/peropero/Desktop/node_modules:/mnt/c/Users/peropero/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase" "$@"
fi
